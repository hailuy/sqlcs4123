SELECT E.ENAME,E.SAL + E.COMM,E1.ENAME
FROM EMPS E,EMPS E1
WHERE (E.SAL + E.COMM) = (E1.SAL + E1.COMM) AND E.EMPNO > E1.EMPNO AND E.COMM IS NOT NULL AND E1.COMM IS NOT NULL
UNION
SELECT E.ENAME,E.SAL,E1.ENAME
FROM EMPS E,EMPS E1
WHERE E.SAL = E1.SAL AND E.EMPNO > E1.EMPNO AND E.COMM IS NULL AND E1.COMM IS NULL
/
